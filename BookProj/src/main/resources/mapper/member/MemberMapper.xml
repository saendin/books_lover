	 <!-- 회원 단건조회 -->
	 <select id="getMember" parameterType="String" resultType="MemberVO">
	 	SELECT * FROM member
	 	WHERE mem_id = #{ memId }
	 </select>
	 
	 <!-- 회원목록조회 -->
	 <select id="getAllMemberList" resultType="MemberVO">
	 	SELECT * FROM member
	 </select>
	 

 
 
 
 </mapper>